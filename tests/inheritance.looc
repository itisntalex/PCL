class Voiture =
(
	var brand : string;
	var	model : string;
	var maxspeed : int;
	
	method init() 
	{
		brand := "?";
		model := "?";
		maxspeed := 0;
	}

	method getSpeed() : int 
	{
		return(maxspeed);
	}

	method setBrand(n : string) 
	{
		brand := n;
	}

	method getBrand() : string
	{
		return(brand);
	}
)

class BugattiVeyron inherit Voiture =
(
	method init()
	{
		do super.init();
		brand := "Bugatti";
		model := "Veyron";
		maxspeed := 431;
	}
)

var v : Voiture;
var b : BugattiVeyron;


v := new Voiture;
b := new BugattiVeyron;

do v.init();
do b.init();
write v.getBrand();

do v.setName("No Brand");

write b.getSpeed();
write v.getBrand();



